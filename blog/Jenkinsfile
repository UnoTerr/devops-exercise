node {  
    stage('Build') { 
        sh 'sudo curl -L --fail https://github.com/docker/compose/releases/download/1.27.4/run.sh -o /usr/local/bin/docker-compose'
        sh 'sudo chmod +x /usr/local/bin/docker-compose'
        sh '/usr/bin/docker-compose build'
    }
    stage('Test') { 
        echo 'BBB'
    }
    stage('Deploy') { 
        echo'111'
    }
}
